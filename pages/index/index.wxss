.swiper{
  height: 340rpx;
}
.banner image{
  width: 100%;
  height: 340rpx;
}
.navs{
  display: flex;
  padding: 20rpx 0;
  justify-content: space-around;
}
.navs image{
  width: 128rpx;
  height: 140rpx;
}
.products{
  margin-top: 20rpx;
}
.products .title{
  height: 60rpx;
  width: 100%;
  margin-bottom: 20rpx;
}
.products .title image{
  width: 100%;
  height: 60rpx;
}
.products .content{
  display: flex;
  justify-content: space-around;
  padding: 0 20rpx;
}
.products .content-left{
  margin-right: 10rpx;
}
.products .content-left image{
  width: 232rpx;
  height: 386rpx;
}
.products .content-right{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
  
}
.products .content-right image{
  width: 230rpx;
  height: 188rpx;
  
}
.products + .products .content-right image{
    width: 193rpx;
}
.products + .products .content-right image:nth-child(3n+1){
  width: 273rpx;
}